
/**
 * Write a description of class ComparadorValor here.
 *
 * @author (your name)
 * @version (a version number or a date)
 */
import java.util.Comparator;

public class ComparadorValor implements Comparator<Fatura> {
    
    public int compare(Fatura f1, Fatura f2) {
        
        if (f1.getValor() < f2.getValor()){
            return 1;
        }
        else if (f1.getValor() > f2.getValor()) { 
            return -1;
        }
        else return 0;        
    }
}
}